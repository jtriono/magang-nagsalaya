DROP TABLE IF EXISTS `nota_retur_jual`;
DROP VIEW IF EXISTS `nota_retur_jual`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `nota_retur_jual` AS select `rp`.`no_retur_penjualan` AS `no_retur_penjualan`,`rp`.`tanggal_retur_penjualan` AS `tanggal_retur_penjualan`,`rp`.`kode_cust` AS `kode_cust`,`rp`.`no_nota_penjualan` AS `no_nota_penjualan`,`rp`.`totalharga_retur_penjualan` AS `totalharga_retur_penjualan`,`drj`.`kode_barang` AS `kode_barang`,`drj`.`nama_barang` AS `nama_barang`,`drj`.`batch` AS `batch`,`drj`.`expired_date` AS `expired_date`,`drj`.`harga_jual` AS `harga_jual`,`drj`.`jumlah_barang` AS `jumlah_barang`,`drj`.`discount_percent` AS `discount_percent`,`drj`.`total_harga` AS `total_harga`,`rp`.`delete` AS `delete`,`drj`.`retur` AS `retur` from (`retur_penjualan` `rp` join `detail_retur_jual` `drj`) where (`rp`.`no_nota_penjualan` = `drj`.`no_nota_penjualan`);

--
-- Definition of view `print_detail_stok`
--

DROP TABLE IF EXISTS `print_detail_stok`;
DROP VIEW IF EXISTS `print_detail_stok`;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`localhost` SQL SECURITY DEFINER VIEW `print_detail_stok` AS select `ds`.`no_nota` AS `no_nota`,`ds`.`kode_barang` AS `kode_barang`,`ds`.`nama_barang` AS `nama_barang`,`ds`.`batch` AS `batch`,`ds`.`expired_date` AS `expired_date`,`ds`.`jumlah_barang` AS `jumlah_barang`,`ds`.`tanggal` AS `tanggal`,`ds`.`keterangan` AS `keterangan`,`ds`.`delete` AS `status`,`b`.`no_nota_pembelian` AS `no_nota_pembelian`,`b`.`tanggal_pembelian` AS `tanggal_pembelian`,`b`.`kode_supplier` AS `kode_supplier`,`b`.`tanggal_jt_pembelian` AS `tanggal_jt_pembelian`,`b`.`total_harga_pembelian` AS `total_harga_pembelian`,`b`.`delete` AS `status_pembelian`,`j`.`no_nota_penjualan` AS `no_nota_penjualan`,`j`.`tanggal_penjualan` AS `tanggal_penjualan`,`j`.`kode_customer` AS `kode_customer`,`j`.`tanggal_jt_penjualan` AS `tanggal_jt_penjualan`,`j`.`total_harga_penjualan` AS `total_harga_penjualan`,`j`.`delete` AS `status_penjualan` from ((`detail_stok` `ds` join `penjualan` `j`) join `pembelian` `b`) where ((`ds`.`no_nota` = `b`.`no_nota_pembelian`) and (`ds`.`no_nota` = `j`.`no_nota_penjualan`));



